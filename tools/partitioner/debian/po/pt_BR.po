#. Type: select
#. Choices
#: ../templates:3
msgid "${DISCS}, Finish"
msgstr "${DISCS}, Finalizar"

#. Type: select
#. Description
#: ../templates:4
msgid "Disk to partition:"
msgstr "Disco a ser particionado :"

#. Type: select
#. Description
#: ../templates:4
msgid "Please choose one of the listed disks, to create partitions on it."
msgstr "Por favor escolha o disco no qual você deseja criar partições."

#. Type: error
#. Description
#: ../templates:9
msgid "No disk found"
msgstr "Nenhum disco encontrado"

#. Type: error
#. Description
#: ../templates:9
msgid ""
"Unable to find any disk in your system. Maybe some kernel modules need to be "
"loaded."
msgstr ""
"Não foi possível encontrar nenhum disco em seu sistema. Talvez alguns "
"módulos do kernel precisem ser carregados."

#. Type: error
#. Description
#: ../templates:15
msgid "Partitioning error"
msgstr "Erro de particionamento"

#. Type: error
#. Description
#: ../templates:15
msgid "Failed to partition the disk ${DISC}."
msgstr "Falha ao particionar o disco ${DISC}."

#. Type: text
#. Description
#. Main menu item
#: ../templates:21
msgid "Partition a hard drive"
msgstr "Particionar um disco rígido"

#~ msgid "Please choose which disc you want to partition"
#~ msgstr "Por favor selecione qual disco você deseja particionar"

