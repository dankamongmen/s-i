msgid "-- South America --"
msgstr "-- อเมริกาใต้ --"

#: ../countrylist:17
msgid "-- Caribbean --"
msgstr "-- แคริบเบียน --"

#: ../countrylist:18
msgid "-- Europe --"
msgstr "-- ยุโรป --"

#: ../countrylist:19
msgid "-- Asia --"
msgstr "-- เอเชีย --"

#: ../countrylist:20
msgid "-- Africa --"
msgstr "-- แอฟริกา --"

#: ../countrylist:21
msgid "-- Indian Ocean --"
msgstr "-- มหาสมุทรอินเดีย --"

#: ../countrylist:22
msgid "-- Oceania --"
