msgstr "-- Det Karibiske Hav --"

#: ../../countrylist:18
msgid "-- Europe --"
msgstr "-- Europa --"

#: ../../countrylist:19
msgid "-- Asia --"
msgstr "-- Asia --"

#: ../../countrylist:20
msgid "-- Africa --"
msgstr "-- Afrika --"

#: ../../countrylist:21
msgid "-- Indian Ocean --"
msgstr "-- Det Indiske Hav --"

#: ../../countrylist:22
