msgstr "-- Централна Америка --"

#: ../../countrylist:16
msgid "-- South America --"
msgstr "-- Јужна Америка --"

#: ../../countrylist:17
msgid "-- Caribbean --"
msgstr "-- Карапски острови --"

#: ../../countrylist:18
msgid "-- Europe --"
msgstr "-- Европа --"

#: ../../countrylist:19
msgid "-- Asia --"
msgstr "-- Азија --"

#: ../../countrylist:20
msgid "-- Africa --"
msgstr "-- Африка --"

#: ../../countrylist:21
msgid "-- Indian Ocean --"
msgstr "-- Индиски океан --"

#: ../../countrylist:22
msgid "-- Oceania --"
msgstr "-- Океанија --"

#: ../../countrylist:23
msgid "-- Antarctica --"
msgstr "-- Антартик --"

