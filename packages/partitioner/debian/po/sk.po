#. Type: select
#. Choices
#: ../partitioner.templates:3
msgid "${DISCS}, Finish"
msgstr "${DISCS}, Koniec"

#. Type: select
#. Description
#: ../partitioner.templates:4
msgid "Disk to partition:"
msgstr "Disk na rozdelenie:"

#. Type: select
#. Description
#: ../partitioner.templates:4
msgid "Please choose one of the listed disks to create partitions on it."
msgstr "Prosím, zvoľte z uvedených diskov ten, na ktorom vytvoríte oblasti."

#. Type: select
#. Description
#: ../partitioner.templates:4
msgid "Select \"Finish\" at the bottom of the list when you are done."
msgstr "Ukončenie potvrdíte stlačením \"Koniec\" v spodnom zozname."

#. Type: error
#. Description
#: ../partitioner.templates:11
msgid "No disk found"
msgstr "Nenašiel sa žiaden disk"

#. Type: error
#. Description
#: ../partitioner.templates:11
msgid ""
"Unable to find any disk in your system. Maybe some kernel modules need to be "
"loaded."
msgstr "Nenašiel sa žiaden disk. Možno sa musia načítať nejaké moduly jadra."

#. Type: error
#. Description
#: ../partitioner.templates:17
msgid "Partitioning error"
msgstr "Chyba pri rozdeľovaní disku"

#. Type: error
#. Description
#: ../partitioner.templates:17
msgid "Failed to partition the disk ${DISC}."
msgstr "Zlyhalo rozdelenie disku ${DISC}."

#. Type: text
#. Description
#. Main menu item
#: ../partitioner.templates:23
msgid "Partition a hard drive"
msgstr "Rozdelenie pevného disku"
