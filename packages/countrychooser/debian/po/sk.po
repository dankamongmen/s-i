#. Type: select
#. Choices
#: ../templates-in:22
msgid "${SHORTLIST}"
msgstr "${SHORTLIST}"

#. Type: select
#. Choices
#: ../templates-in:22 ../countrylist:25
msgid "other"
msgstr "iná"

#. Type: select
#. Description
#: ../templates-in:23
msgid "Choose a country, territory or area:"
msgstr "Zvoľte krajinu, územie alebo oblasť:"

#. Type: select
#. Description
#: ../templates-in:23
msgid ""
"Based on your language, you are probably located in one of these countries "
"or regions."
msgstr ""
"Podľa vášho jazyka by ste sa mali nachádzať v jednej z nasledovných krajín "
"alebo oblastí."

#. Type: text
#. Description
#. Main menu entry
#. This is kept in case we decide to go back to "choose country"
#. Keeping this, we won't waste translations
#: ../templates-in:40
msgid "Choose country"
msgstr "Výber krajiny"

#. Type: text
#. Description
#. Main menu entry
#: ../templates-in:45
msgid "Choose country or region"
msgstr "Výber krajiny alebo oblasti"

#: ../countrylist:15
msgid "North America"
msgstr "Severná Amerika"

#: ../countrylist:16
msgid "Central America"
msgstr "Stredná Amerika"

#: ../countrylist:17
msgid "South America"
msgstr "Južná Amerika"

#: ../countrylist:18
