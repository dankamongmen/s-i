#. Type: select
#. Choices
#: ../templates-in:22
msgid "${SHORTLIST}"
msgstr "${SHORTLIST}"

#. Type: select
#. Choices
#: ../templates-in:22 ../countrylist:25
msgid "other"
msgstr "Muu"

#. Type: select
#. Description
#: ../templates-in:23
msgid "Choose a country, territory or area:"
msgstr "Valitse maa tai alue:"

#. Type: select
#. Description
#: ../templates-in:23
msgid ""
"Based on your language, you are probably located in one of these countries "
"or regions."
msgstr ""
"Valitsemasi kielen perusteella olet luultavasti jossakin näistä maista tai "
"alueista."

#. Type: text
#. Description
#. Main menu entry
#. This is kept in case we decide to go back to "choose country"
#. Keeping this, we won't waste translations
#: ../templates-in:40
msgid "Choose country"
msgstr "Valitse maa"

#. Type: text
#. Description
#. Main menu entry
#: ../templates-in:45
msgid "Choose country or region"
msgstr "Valitse maa tai alue"

