#. Type: select
#. Choices
#: ../templates-in:22
msgid "${SHORTLIST}"
msgstr "${SHORTLIST}"

#. Type: select
#. Choices
#: ../templates-in:22 ../countrylist:25
msgid "other"
msgstr "其它"

#. Type: select
#. Description
#: ../templates-in:23
msgid "Choose a country, territory or area:"
msgstr "請選擇一個國家、領域或地區："

#. Type: select
#. Description
#: ../templates-in:23
msgid ""
"Based on your language, you are probably located in one of these countries "
"or regions."
msgstr "根據您的語言，您可能位於以下的國家或地區之一。"

#. Type: text
#. Description
#. Main menu entry
#. This is kept in case we decide to go back to "choose country"
#. Keeping this, we won't waste translations
#: ../templates-in:40
msgid "Choose country"
msgstr "選擇國家"

#. Type: text
#. Description
#. Main menu entry
#: ../templates-in:45
msgid "Choose country or region"
msgstr "選擇國家或地區"

#: ../countrylist:15
msgid "North America"
msgstr "北美洲"

#: ../countrylist:16
msgid "Central America"
msgstr "中美洲"

#: ../countrylist:17
msgid "South America"
msgstr "南美洲"

#: ../countrylist:18
msgid "Caribbean"
msgstr "加勒比海"

#: ../countrylist:19
msgid "Europe"
msgstr "歐洲"

#: ../countrylist:20
msgid "Asia"
msgstr "亞洲"

#: ../countrylist:21
msgid "Africa"
msgstr "非洲"

#: ../countrylist:22
msgid "Indian Ocean"
msgstr "印度洋"

#: ../countrylist:23
msgid "Oceania"
msgstr "大洋洲"

#: ../countrylist:24
msgid "Antarctica"
msgstr "南極洲"

#~ msgid ""
#~ "The country choice here will change the default values for the location "
#~ "and the keyboard layout."
#~ msgstr "國家的選擇會影響地理位置和鍵盤布局的默認值。"

#~ msgid ""
#~ "The locale settings will not be changed from their current value "
#~ "(${DEFAULTLOCALE})."
#~ msgstr "目前的區域設置(${DEFAULTLOCALE})并不會被改變。"

#~ msgid ""
#~ "The country choice here will both change the locale and defaults for "
#~ "location and keyboard layout."
#~ msgstr ""
#~ "國家的選擇既會影響區域設置(locale)也會改變地理位置和鍵盤布局的默認值。"

#~ msgid ""
#~ "Based on your current language selection, the following countries are "
#~ "fully supported. This means that the locale obtained by combining the "
#~ "language and one of these countries is supported by Debian."
#~ msgstr ""
#~ "根據您目前所選擇的語言，以下國家是被完全支持的。也就是說，Debian 支持由此"
#~ "種語言與以下國家之一組合而成的區域設置(locale)。"

#, fuzzy
#~ msgid ""
