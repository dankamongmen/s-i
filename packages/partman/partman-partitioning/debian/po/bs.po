#. Type: text
#. Description
#: ../templates:3
msgid "Resizing partition..."
msgstr "Mijenjam veličinu particije..."

#. Type: text
#. Description
#: ../templates:7
msgid "Copying partition..."
msgstr "Kopiram particiju..."

#. Type: text
#. Description
#: ../templates:11
msgid "Computing the new state of the partition table..."
msgstr "Izračunavam novo stanje particione tabele..."

#. Type: boolean
#. Description
#: ../templates:15
msgid "Write the changes to disk devices and copy the data?"
msgstr "Zapiši promjene na diskovne uređaje i kopiraj podatke?"

#. Type: boolean
#. Description
#: ../templates:15
msgid "Before the copy operation takes place, the changes have to be written to disk."
msgstr "Prije nego počne operacija kopiranja, promjene trebaju biti zapisane na diskovne uređaje."

#. Type: select
#. Description
#: ../templates:24
msgid "Source partition:"
msgstr "Izvorna particija:"

#. Type: select
#. Description
#: ../templates:24
msgid "Please choose the partition which contains the data you want to copy."
msgstr "Molim odaberite particiju koja sadrži podatke koje želite kopirati."

#. Type: error
#. Description
#: ../templates:29
msgid "Copy operation failure"
msgstr "Operacija kopiranja neuspješna"

#. Type: error
#. Description
#: ../templates:29
msgid "The copy operation is aborted."
msgstr "Operacija kopiranja je obustavljena."

#. Type: boolean
#. Description
#: ../templates:36
msgid "Write the changes to disk and resize the partition?"
msgstr "Zapisati promjene na diskovne uređaje i promjeniti veličinu particije?"

#. Type: boolean
#. Description
#: ../templates:36
msgid "Before the resize operation takes place, the changes have to be written to disk."
msgstr "Prije nego počne operacija promjene veličine, promjene moraju biti zapisane na disk."

#. Type: boolean
#. Description
#: ../templates:36
msgid "You cannot undo this operation."
msgstr "Nećete moći poništiti ovu operaciju."

#. Type: string
#. Description
#: ../templates:45
msgid "The minimum size you can use is ${MINSIZE} or ${PERCENT} and the maximum size is ${MAXSIZE}."
msgstr "Najmanja veličina koju možete koristiti je ${MINSIZE} ili ${PERCENT}, a najveća veličina je ${MAXSIZE}."

#. Type: error
#. Description
#: ../templates:59
msgid "Too large size"
msgstr "Prevelika veličina"

#. Type: error
#. Description
#: ../templates:63
msgid "Too small size"
msgstr "Premala veličina"

#. Type: error
#. Description
#: ../templates:67
msgid "Resize operation failure"
msgstr "Operacija promjene veličine neuspješna"

#. Type: error
#. Description
#: ../templates:67
msgid "An error occurred while writing the changes to the storage devices."
msgstr "Došlo je do greške pri zapisivanju promjena na diskovne uređaje."

#. Type: error
#. Description
#: ../templates:67
msgid "The resize operation is aborted."
msgstr "Operacija promjene veličine je obustavljena."

#. Type: string
#. Description
#: ../templates:75
msgid "New partition size:"
msgstr "Nova veličina particije:"

#. Type: string
#. Description
#: ../templates:75
msgid "The maximum size you can use is ${MAXSIZE}."
msgstr "Najveća veličina koju možete koristiti je ${MAXSIZE}."

#. Type: string
#. Description
#: ../templates:75
#, no-c-format
msgid "Hint: Use \"20%\" (or \"30%\", etc.) for 20% (resp. 30%, etc.) of the available free space for this partition.  Use \"max\" as a shortcut for the maximum allowed size."
msgstr "Savjet: Koristite \"20%\" (ili \"30%\", itd.) za 20% (odg. 30%, itd.) dostupnog slobodnog prostora za ovu particiju.  Koristite \"max\" kao prečicu za najveću dozvoljenu veličinu."

#. Type: error
#. Description
#: ../templates:84
msgid "Invalid size"
msgstr "Neispravna veličina"

#. Type: select
#. Choices
#: ../templates:88
msgid "Beginning"
msgstr "Početak"

#. Type: select
#. Choices
#: ../templates:88
msgid "End"
msgstr "Kraj"

#. Type: select
#. Description
#: ../templates:89
msgid "Location for the new partition:"
msgstr "Lokacija nove particije:"

#. Type: select
#. Description
#: ../templates:89
msgid "Please choose whether you want the new partition to be created at the beginning or at the end of the available space."
msgstr "Molim odaberite da li želite da nova particija bude kreirana na početku ili na kraju dostupnog prostora."

#  Type: select
#  Description
#. Type: select
#. Description
#: ../templates:96
msgid "File system for the new partition:"
msgstr "Datotečni sistem za novu particiju:"

#. Type: select
#. Choices
#: ../templates:100
msgid "Primary"
msgstr "Primarna"

#. Type: select
#. Choices
#: ../templates:100
msgid "Logical"
msgstr "Logička"

#  Type: select
#  Description
#. Type: select
#. Description
#: ../templates:101
msgid "Type for the new partition:"
msgstr "Tip za novu particiju:"

#. Type: multiselect
#. Description
#: ../templates:106
msgid "Flags for the new partition:"
msgstr "Opcije za novu particiju:"

#. Type: string
#. Description
#: ../templates:110
msgid "Partition name:"
msgstr "Naziv particije:"

#. Type: select
#. Description
#: ../templates:115
msgid "Partition table type:"
msgstr "Tip particione tabele:"

#. Type: select
#. Description
#: ../templates:115
msgid "Select the type of partition table to use."
msgstr "Izaberite tip particione tabele koji će se koristiti."

#. Type: text
#. Description
#: ../templates:120
msgid "Set the partition flags"
msgstr "Postavi particione opcije"

#. Type: text
#. Description
#: ../templates:124
msgid "Name:"
msgstr "Naziv:"

#. Type: text
#. Description
#: ../templates:128
msgid "Bootable flag:"
msgstr "Bootable opcija:"

#. Type: text
#. Description
#: ../templates:132
msgid "on"
msgstr "uključena"

#. Type: text
#. Description
#: ../templates:136
msgid "off"
msgstr "isključena"

#. Type: text
#. Description
#: ../templates:140
msgid "Size:"
msgstr "Veličina:"

#. Type: text
#. Description
#: ../templates:144
msgid "Copy data from another partition"
msgstr "Kopiraj podatke sa druge particije"

#  Type: text
#  Description
#. Type: text
#. Description
#: ../templates:148
msgid "Delete the partition"
msgstr "Obriši particiju"

#. Type: text
#. Description
#: ../templates:152
msgid "Create a new partition"
msgstr "Kreiraj novu particiju"

#. Type: text
#. Description
#: ../templates:156
msgid "Create a new empty partition table on this device"
msgstr "Kreiraj novu praznu particionu tabelu na ovom uređaju"

