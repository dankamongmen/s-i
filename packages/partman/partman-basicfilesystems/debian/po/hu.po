#. Choices
#. Note to translators : Please keep your translations of the choices
#. below a 65 columns limit (which means 65 characters
#. in single-byte languages) including the initial path
#: ../partman-basicfilesystems.templates:83
msgid "/ - the root file system"
msgstr "/ - a gyökérfájlrendszer"

#. Type: select
#. Choices
#: ../partman-basicfilesystems.templates:83
msgid "/boot - static files of the boot loader"
msgstr "/boot - a rendszerbetöltő statikus fájljai"

#. Type: select
#. Choices
#: ../partman-basicfilesystems.templates:83
msgid "/home - user home directories"
msgstr "/home - felhasználói könyvtárak"

#. Type: select
#. Choices
#: ../partman-basicfilesystems.templates:83
msgid "/tmp - temporary files"
msgstr "/tmp - átmeneti fájlok"

#. Type: select
#. Choices
#: ../partman-basicfilesystems.templates:83
msgid "/usr - static data"
msgstr "/usr - statikus adatok"

#. Type: select
#. Choices
#: ../partman-basicfilesystems.templates:83
msgid "/var - variable data"
msgstr "/var - változékony adatok"

#. Type: select
#. Choices
#: ../partman-basicfilesystems.templates:83
msgid "/srv - data for services provided by this system"
msgstr "/srv - a rendszer nyújtotta szolgáltatások adatai"

#. Type: select
#. Choices
#: ../partman-basicfilesystems.templates:83
msgid "/opt - add-on application software packages"
msgstr "/opt - külső alkalmazásszoftver-csomagok"

#. Type: select
#. Choices
#: ../partman-basicfilesystems.templates:83
msgid "/usr/local - local hierarchy"
msgstr "/usr/local - saját fordítású szoftverek hierarchiája"

#. Type: select
#. Choices
#: ../partman-basicfilesystems.templates:88
msgid "/dos"
msgstr "/dos"

#. Type: select
#. Choices
#: ../partman-basicfilesystems.templates:88
msgid "/windows"
msgstr "/windows"

#. Type: select
#. Choices
#: ../partman-basicfilesystems.templates:88
msgid "Enter manually"
msgstr "Kézi megadás"

#. Type: select
#. Choices
#: ../partman-basicfilesystems.templates:88
msgid "Do not mount it"
msgstr "Ne csatlakoztassa fel"

#. Type: string
#. Description
#: ../partman-basicfilesystems.templates:93
msgid "Mount point for this partition:"
msgstr "E partíció csatlakoztatási pontja:"

#. Type: error
#. Description
#: ../partman-basicfilesystems.templates:97
msgid "Invalid mount point"
msgstr "Érvénytelen csatlakoztatási pont"

#. Type: error
#. Description
#: ../partman-basicfilesystems.templates:97
msgid "The mount point you entered is invalid."
msgstr "A megadott csatlakoztatási pont érvénytelen."

#. Type: error
#. Description
#: ../partman-basicfilesystems.templates:97
msgid "Mount points must start with \"/\". They cannot contain spaces."
msgstr ""
"Minden csatlakoztatási pont neve perjellel (/) kezdődik. A névben nem állhat "
"szóköz."

#. Type: string
#. Description
#: ../partman-basicfilesystems.templates:104
msgid "Label for the file system in this partition:"
msgstr "A partíción levő fájlrendszer címkéje:"

#. Type: text
#. Description
#: ../partman-basicfilesystems.templates:108
msgid "Format the swap area:"
msgstr "Cserehely megformázása:"

#. Type: text
#. Description
#. In the following context: "Format the partition: yes"
#: ../partman-basicfilesystems.templates:113
msgid "yes"
msgstr "igen"

#. Type: text
#. Description
#. In the following context: "Format the partition: no"
#: ../partman-basicfilesystems.templates:118
msgid "no"
msgstr "nem"

#. Type: text
#. Description
#. label of file system
#: ../partman-basicfilesystems.templates:123
msgid "Label:"
msgstr "Címke:"

#. Type: text
#. Description
#. Up to 24 character positions
#: ../partman-basicfilesystems.templates:133
msgid "Reserved blocks:"
msgstr "Fenntartott blokkok:"

#. Type: string
#. Description
#: ../partman-basicfilesystems.templates:137
msgid "Percentage of the file system blocks reserved for the super-user:"
msgstr "Fájlrendszer blokkjainak rendszergazda számára fenntartott százaléka:"

#. Type: text
#. Description
#. Up to 25 character positions
#: ../partman-basicfilesystems.templates:142
msgid "Typical usage:"
msgstr "Jellemző felhasználás:"

#. Type: text
#. Description
#. In the following context: "Typical usage: standard"
#: ../partman-basicfilesystems.templates:147
msgid "standard"
msgstr "szokásos"

#. Type: select
#. Description
#: ../partman-basicfilesystems.templates:155
msgid "Typical usage of this partition:"
msgstr "E partíció jellemző felhasználása:"

#. Type: select
#. Description
#: ../partman-basicfilesystems.templates:155
msgid ""
"Please specify how the file system is going to be used, so that optimal file "
"system parameters can be chosen for that use."
msgstr ""
"Jellemezze a fájlrendszer tipikusnak szánt felhasználását, ezzel választva "
"ki fájlrendszer-paraméterek lehetséges értékei közül a kívánt célnak "
"legmegfelelőbbeket."

#. Type: select
#. Description
#: ../partman-basicfilesystems.templates:155
msgid ""
"standard = standard parameters, news = one inode per 4KB block, largefile = "
"one inode per megabyte, largefile4 = one inode per 4 megabytes."
msgstr ""
"standard = szokásos paraméterek, news = 4kB-s blokkonként egy inode,"
"largefile = 1MB-onként egy inode, largefile4 = 4MB-onként egy inode"

#. Type: text
#. Description
#. This is an item in the menu "Partition settings"
#: ../partman-basicfilesystems.templates:167
msgid "Mount point:"
msgstr "Csatlakoztatási pont:"

#. Type: text
#. Description
#. In the following context: "Mount point: none"
#: ../partman-basicfilesystems.templates:172
msgid "none"
msgstr "semmi"

#. Type: text
#. Description
#: ../partman-basicfilesystems.templates:176
msgid "Ext2 file system"
msgstr "Ext2 fájlrendszer"

#. Type: text
#. Description
#. Short file system name (untranslatable in many languages)
#: ../partman-basicfilesystems.templates:181
msgid "ext2"
msgstr "ext2"

#. Type: text
#. Description
#: ../partman-basicfilesystems.templates:185
msgid "FAT16 file system"
msgstr "FAT16 fájlrendszer"

#. Type: text
#. Description
#. Short file system name (untranslatable in many languages)
#: ../partman-basicfilesystems.templates:190
msgid "fat16"
msgstr "fat16"

#. Type: text
#. Description
#: ../partman-basicfilesystems.templates:194
msgid "FAT32 file system"
msgstr "FAT32 fájlrendszer"

#. Type: text
#. Description
#. Short file system name (untranslatable in many languages)
#: ../partman-basicfilesystems.templates:199
msgid "fat32"
msgstr "fat32"

#. Type: text
#. Description
#: ../partman-basicfilesystems.templates:212
msgid "swap area"
msgstr "cserehely"

#. Type: text
#. Description
#. Short variant of `swap space'
#: ../partman-basicfilesystems.templates:217
msgid "swap"
msgstr "swap"

#. Type: multiselect
#. Choices
#. Note to translators : Please keep your translations of the choices
#. below a 65 columns limit (which means 65 characters
#. in single-byte languages)
#: ../partman-basicfilesystems.templates:228
msgid "noatime - do not update inode access times at each access"
msgstr "noatime - inode-ok legutóbbi elérési idejének feljegyzése elmarad"

#. Type: multiselect
#. Choices
#: ../partman-basicfilesystems.templates:228
msgid "nodev - do not support character or block special devices"
msgstr ""
"nodev - speciális (karakteres és blokkos) eszközfájlok támogatásának "
"mellőzése"

#. Type: multiselect
#. Choices
#: ../partman-basicfilesystems.templates:228
msgid "nosuid - ignore set-user-identifier or set-group-identifier bits"
msgstr ""
"nosuid - fájlokon található felhasználóiazonosító- illetve csoportazonosító-"
"kényszerítő bitek figyelmen kívül hagyása"

#. Type: multiselect
#. Choices
#: ../partman-basicfilesystems.templates:228
msgid "noexec - do not allow execution of any binaries"
msgstr "noexec - futtatható bináris fájlok futtatásának tiltása"

#. Type: multiselect
#. Choices
#: ../partman-basicfilesystems.templates:228
msgid "usrquota - user disk quota accounting enabled"
msgstr "usrquota - felhasználók lemezhasználati mértékének naplózása"

#. Type: multiselect
#. Choices
#: ../partman-basicfilesystems.templates:228
msgid "grpquota - group disk quota accounting enabled"
msgstr "grpquota - felhasználói csoportok lemezhasználati mértékének naplózása"

#. Type: multiselect
#. Choices
#: ../partman-basicfilesystems.templates:228
msgid "user_xattr - support user extended attributes"
msgstr ""

#. Type: multiselect
#. Choices
#. Note to translators : Please keep your translations of the choices
#. below a 65 columns limit (which means 65 characters
#. in single-byte languages) including the initial path
#: ../partman-basicfilesystems.templates:237
msgid "ro - mount the file system read-only"
msgstr "ro - fájlrendszer felcsatlakoztatása csak olvasható módban"

#. Type: multiselect
#. Choices
#: ../partman-basicfilesystems.templates:237
msgid "sync - all input/output activities occur synchronously"
msgstr ""
"sync - a fájlrendszeren történő minden bemeneti/kimeneti művelet szinkron "
"módon lesz végrehajtva"

#. Type: multiselect
#. Choices
#: ../partman-basicfilesystems.templates:237
msgid "quiet - changing owner and permissions does not return errors"
msgstr ""
"quiet - fájlok tulajdonosának és jogosultságainak megváltoztatásakor "
"felmerülő hibák nem eredményeznek hibaüzeneteket"

#. Type: multiselect
#. Description
#: ../partman-basicfilesystems.templates:238
msgid "Mount options:"
msgstr "Csatlakoztatási kapcsolók:"

#. Type: multiselect
#. Description
#: ../partman-basicfilesystems.templates:238
msgid "Mount options can tune the behavior of the file system."
msgstr ""
"A felcsatlakoztatási kapcsolók a fájlrendszer viselkedésének hangolását "
"szolgálják."
