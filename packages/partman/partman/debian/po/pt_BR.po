#. Description
#: ../partman.templates:268
#, no-c-format
msgid "SCSI%s (%s,%s,%s), partition #%s (%s)"
msgstr "SCSI%s (%s,%s,%s), partição #%s (%s)"

#. Type: text
#. Description
#: ../partman.templates:272
#, no-c-format
msgid "RAID%s device #%s"
msgstr "Dispositivo RAID%s #%s"

#. Type: text
#. Description
#: ../partman.templates:276
#, no-c-format
msgid "LVM VG %s, LV %s"
msgstr "VG LVM %s, LV %s"

#. Type: text
#. Description
#: ../partman.templates:280
msgid "Cancel this menu"
msgstr "Cancelar este menu"

#. Type: text
