msgstr "IDE%s πρωτεύων (%s)"

#. Type: text
#. Description
#. For example IDE1 slave (hdd)
#: ../partman-base.templates:231
#, no-c-format
msgid "IDE%s slave (%s)"
msgstr "IDE%s δευτερεύων (%s)"

#. Type: text
#. Description
#. For example IDE1 master, partition #5 (hdc5)
#: ../partman-base.templates:236
#, no-c-format
msgid "IDE%s master, partition #%s (%s)"
msgstr "IDE%s πρωτεύων, κατάτμηση #%s (%s)"

#. Type: text
#. Description
#. For example IDE2 slave, partition #5 (hdf5)
#: ../partman-base.templates:241
#, no-c-format
msgid "IDE%s slave, partition #%s (%s)"
msgstr "IDE%s δευτερεύων, κατάτμηση #%s (%s)"

#. Type: text
#. Description
#: ../partman-base.templates:245
#, no-c-format
