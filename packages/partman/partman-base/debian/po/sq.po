#. Type: text
#. Description
#: ../partman-base.templates:257
#, no-c-format
msgid "Encrypted volume (%s)"
msgstr "Volumi i kriptuar (%s)"

#. Type: text
#. Description
#: ../partman-base.templates:261
#, no-c-format
msgid "LVM VG %s, LV %s"
msgstr "LVM VG %s, LV %s"

#. Type: text
#. Description
