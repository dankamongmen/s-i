#. Type: text
#. Description
#: ../partman-lvm.templates:3
msgid "Unallocated physical volumes:"
msgstr "Ikke-inddraget fysisk bind:"

#. Type: text
#. Description
#: ../partman-lvm.templates:7
msgid "Volume groups:"
msgstr "Bindgrupper:"

#. Type: text
#. Description
#: ../partman-lvm.templates:11
msgid "Uses physical volume:"
msgstr "Benytter fysisk bind:"

