msgid "Erase entire disk: ${DEVICE}"
msgstr "Volledige schijf wissen: ${DEVICE}"

#  Type: text
#  Description
#. Type: text
#. Description
#: ../partman-auto.templates:80
msgid "Manually edit partition table"
msgstr "Schijfindeling handmatig bepalen"

#  Type: text
#  Description
#  Type: text
#  Description
#. Type: text
#. Description
#: ../partman-auto.templates:84
msgid "Automatically partition the free space"
msgstr "De beschikbare vrije ruimte automatisch indelen"

#  Type: text
#  Description
#. Type: text
#. Description
#: ../partman-auto.templates:88
msgid "All files in one partition (recommended for new users)"
msgstr "Alles in één partitie (aanbevolen voor nieuwe gebruikers)"

#. Type: text
#. Description
#: ../partman-auto.templates:92
msgid "Desktop machine"
msgstr "Desktopmachine"

#. Type: text
