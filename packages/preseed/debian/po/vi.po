msgid "Failed to run preseeded command"
msgstr "Không thực thi được lệnh đã chèn trước."

# Type: error
# Description
#. Type: error
#. Description
#: ../preseed-common.templates:45
msgid ""
"Execution of preseeded command \"${COMMAND}\" failed with exit code ${CODE}."
msgstr ""
"Không thực hiện lệnh đã chèn trước \"${COMMAND} được: mã thoát ${CODE}."

