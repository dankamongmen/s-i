d-i netcfg/choose_interface select eth0
d-i netcfg/get_ipaddress string 192.168.0.100
d-i netcfg/get_netmask string 255.255.0.0
d-i netcfg/get_gateway string 192.168.0.1
d-i netcfg/get_nameservers string 192.168.0.1 192.168.0.2
d-i netcfg/confirm_static boolean true
d-i netcfg/disable_dhcp boolean true
d-i netcfg/get_hostname string n2100
d-i netcfg/get_domain string daka.thg.se
