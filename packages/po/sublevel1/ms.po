msgid "Currently configured network parameters:"
msgstr "Parameter konfigurasi rangkaian semasa:"

#. Type: boolean
#. Description
#. :sl1:
#: ../netcfg-static.templates:7001
msgid ""
" interface     = ${interface}\n"
" ipaddress     = ${ipaddress}\n"
" netmask       = ${netmask}\n"
" gateway       = ${gateway}\n"
" pointopoint   = ${pointopoint}\n"
" nameservers   = ${nameservers}"
msgstr ""
" antaramuka = ${interface}\n"
" alamatip = ${ipaddress}\n"
" netmask = ${netmask}\n"
" pintupenghalaan = ${gateway}\n"
" titikketitik = ${pointopoint}\n"
" namapelayan = ${nameservers}"

