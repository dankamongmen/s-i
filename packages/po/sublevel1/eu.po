msgid "-- North America --"
msgstr "-- Ipar Amerika --"

#: ../../countrylist:15
msgid "-- Central America --"
msgstr "-- Erdialdeko Amerika --"

#: ../../countrylist:16
msgid "-- South America --"
msgstr "-- Hego Amerika --"

#: ../../countrylist:17
msgid "-- Caribbean --"
msgstr "-- Karibea --"

#: ../../countrylist:18
