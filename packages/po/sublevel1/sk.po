#. Type: text
#. Description
#. progress bar item
#: ../partman-base.templates:180 ../partman-auto.templates:3
#: ../clock-setup.templates:22
msgid "Please wait..."
msgstr "Čakajte, prosím..."

#. Type: text
#. Description
#: ../partman-base.templates:184
msgid "Partitions formatting"
msgstr "Na oblastiach sa vytvárajú súborové systémy"

#. Type: text
#. Description
#: ../partman-base.templates:188
msgid "Processing..."
msgstr "Prebieha spracovanie..."

#. Type: text
#. Description
#: ../partman-base.templates:200
msgid "Done setting up the partition"
msgstr "Nastavenie oblasti ukončené"

#. Type: text
#. Description
#: ../partman-base.templates:204
msgid "Finish partitioning and write changes to disk"
msgstr "Ukončiť rozdeľovanie a zapísať zmeny na disk"

#. Type: text
#. Description
#: ../partman-base.templates:208
msgid "Undo changes to partitions"
msgstr "Vrátiť späť zmeny na oblastiach"

#. Type: text
#. Description
#: ../partman-base.templates:212
msgid "Show Cylinder/Head/Sector information"
msgstr "Ukázať informácie o Cylindri/Hlave/Sektore"

#. Type: text
#. Description
#: ../partman-base.templates:216
#, no-c-format
msgid "Dump partition info in %s"
msgstr "Vypísať informácie o oblasti do %s"

#. Type: text
#. Description
#. Keep short
#: ../partman-base.templates:221
msgid "FREE SPACE"
msgstr "VOĽNÉ MIESTO"

#. Type: text
#. Description
#. "unusable free space".  No more than 8 symbols.
#: ../partman-base.templates:226
msgid "unusable"
msgstr "nepouž."

#. Type: text
#. Description
#. "primary partition".  No more than 8 symbols.
#: ../partman-base.templates:231
msgid "primary"
msgstr "primárna"

#. Type: text
#. Description
#. "logical partition".  No more than 8 symbols.
#: ../partman-base.templates:236
msgid "logical"
msgstr "logická"

#. Type: text
#. Description
#. "primary or logical".  No more than 8 symbols.
#: ../partman-base.templates:241
msgid "pri/log"
msgstr "pri/log"

#. Type: text
#. Description
#. How to print the partition numbers in your language
#. Examples:
#. %s.
#. No %s
#. N. %s
